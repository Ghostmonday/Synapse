{
  "name": "sinaps-mvp",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "clean": "turbo clean && find . -name node_modules -type d -prune -exec rm -rf {} +",
    "validate": "turbo typecheck && turbo lint"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.25",
    "@types/ws": "^8.5.9",
    "depcheck": "^1.4.7",
    "knip": "^5.69.1",
    "tsx": "^4.20.6",
    "turbo": "^2.3.3",
    "typescript": "^5.9.3",
    "unimported": "^1.31.0"
  },
  "overrides": {
    "livekit-server-sdk": "2.2.0",
    "@supabase/supabase-js": "2.38.1"
  }
}
